<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>agregarManualServlet</servlet-name>
        <servlet-class>controller.agregarManualServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>controller.UsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MenuUsuarioServlet</servlet-name>
        <servlet-class>controller.MenuUsuarioServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministrarRolServlet</servlet-name>
        <servlet-class>controller.AdministrarRolServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministrarPerfilServlet</servlet-name>
        <servlet-class>controller.AdministrarPerfilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministrarDocumentoServlet</servlet-name>
        <servlet-class>controller.AdministrarDocumentoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteEspecificoServlet</servlet-name>
        <servlet-class>controller.ReporteEspecificoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteGlobalServlet</servlet-name>
        <servlet-class>controller.ReporteGlobalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteDescargasServlet</servlet-name>
        <servlet-class>controller.ReporteDescargasServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>agregarManualServlet</servlet-name>
        <url-pattern>/agregarManual</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/Usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenuUsuarioServlet</servlet-name>
        <url-pattern>/MenuUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministrarRolServlet</servlet-name>
        <url-pattern>/AdministrarRol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministrarPerfilServlet</servlet-name>
        <url-pattern>/AdministrarPerfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministrarDocumentoServlet</servlet-name>
        <url-pattern>/AdministrarDocumento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteEspecificoServlet</servlet-name>
        <url-pattern>/ReporteEspecifico</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteGlobalServlet</servlet-name>
        <url-pattern>/ReporteGlobal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteDescargasServlet</servlet-name>
        <url-pattern>/ReporteDescargas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            15
        </session-timeout>
    </session-config>
</web-app>
